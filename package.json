  "bugs": {
    "url": "https://github.com/Chasbob/personal-site/issues"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-netlify": "^2.3.16",
    "gatsby-plugin-offline": "^3.3.2",
    "gatsby-plugin-preact": "^4.2.0",
    "gatsby-plugin-preconnect": "^1.1.54",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
    "gatsby-plugin-sass": "^2.3.14",
    "gatsby-plugin-sitemap": "^2.7.0",
    "gatsby-react-router-scroll": "^3.0.15",
    "gatsby-source-contentful": "^4.1.1",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-remark": "^2.10.0",
    "gatsby-transformer-yaml": "^2.6.0",
    "preact": "^10.5.7",
    "preact-render-to-string": "^5.1.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.0.0",
    "react-icons": "^4.1.0",
    "sharp": "^0.26.2",
    "simple-react-lightbox": "^3.3.4-2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-react-jsx-source": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "bulma": "^0.9.1",
    "bulmaswatch": "^0.8.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^2.20.25",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.17",
    "husky": ">=4",
    "lighthouse": "^6.4.1",
    "lint-staged": ">=10",
    "netlify-cli": "^2.69.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "yargs-parser": "^20.2.4"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "homepage": "https://github.com/Chasbob/personal-site#readme",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chasbob/personal-site.git"
  },
  "scripts": {
    "dev": "yarn develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.md' 'bin/*.js'",
    "build": "gatsby build",
    "serve": "NODE_ENV=production gatsby serve",
    "deploy": "netlify deploy --build --dir public/",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "clean": "gatsby clean",
    "lighthouse": "lighthouse http://localhost:9000 --view"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.js": "prettier --write"
  }
}
