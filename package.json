  "bugs": {
    "url": "https://github.com/Chasbob/personal-site/issues"
  },
  "dependencies": {
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-feed": "^2.11.0",
    "gatsby-plugin-layout": "^1.8.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-netlify": "^2.9.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-preact": "^4.5.0",
    "gatsby-plugin-preconnect": "^1.1.54",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
    "gatsby-plugin-sass": "^3.0.0",
    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-plugin-sitemap": "^2.10.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-react-router-scroll": "^3.5.0",
    "gatsby-remark-images": "^3.9.0",
    "gatsby-source-contentful": "^4.4.0",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-transformer-remark": "^2.14.0",
    "gatsby-transformer-yaml": "^2.9.0",
    "preact": "^10.5.9",
    "preact-render-to-string": "^5.1.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.0.0",
    "react-icons": "^4.1.0",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx-source": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "bulma": "^0.9.1",
    "bulma-css-vars": "^0.5.6",
    "bulmaswatch": "^0.8.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^2.30.1",
    "husky": ">=4.3.7",
    "lighthouse": "^7.0.0",
    "lint-staged": ">=10.5.3",
    "netlify-cli": "^2.70.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.32.2",
    "yargs-parser": "^20.2.4"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "homepage": "https://github.com/Chasbob/personal-site#readme",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chasbob/personal-site.git"
  },
  "scripts": {
    "dev": "yarn develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write .",
    "build": "gatsby build",
    "serve": "NODE_ENV=production gatsby serve",
    "deploy": "netlify deploy --build --dir public/",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "clean": "gatsby clean",
    "lighthouse": "lighthouse http://localhost:9000 --view",
    "update-bulma-colors": "bulma-css-vars"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,scss,css,md,yaml}": "prettier --write"
  }
}
